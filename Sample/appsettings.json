{
  "StorageContext": {
    "AutoMigrate": true
  },
  "Cache": {
    "DefaultDistgroup": "Default",
    "DefaultDistType": "Redis",
    "DistGroups": {
      "Redis": {
        "Default": {
          "Configuration": "127.0.0.1:6379",
          "InstanceName": "Instance1_"
        },
        "Auth": {
          "Configuration": "127.0.0.1:6379",
          "InstanceName": "SharedAuth1_"
        },
        "Big": {
          "Configuration": "127.0.0.1:6379",
          "InstanceName": "Instance3_"
        }
      }
    },
    "Items": {
      "ContextData": {
        "Key": "context_{sid}",
        "DistSeconds": 14400,
        "MemSeconds": 180,
        "DistGroup": "Auth",
        "Note": "Thông tin phiên đăng nhập. Được khởi tạo tại AuthApi"
      },
      "NoDistAndMemSeconds": {
        "Key": "conn_{dbid}",
        "Note": "Chuỗi kết nối vào dữ liệu"
      },
      "NoDist": {
        "Key": "conn_{dbid}",
        "MemSeconds": 14400,
        "Note": "Chuỗi kết nối vào dữ liệu"
      },
      "NoKey": {
        "Key": "",
        "MemSeconds": 14400,
        "Note": "Chuỗi kết nối vào dữ liệu"
      },
      "NoMem": {
        "Key": "index_{custom}",
        "DistSeconds": 14400,
        "Note": "Nội dung html trả về client"
      },
      "DistAndMem": {
        "Key": "context_{sid}",
        "DistSeconds": 14400,
        "MemSeconds": 180,
        "DistGroup": "Auth",
        "Note": "Thông tin phiên đăng nhập. Được khởi tạo tại AuthApi"
      },
      "DistAndMemNoExpire": {
        "Key": "context_{sid}",
        "DistGroup": "Auth",
        "Note": "Thông tin phiên đăng nhập. Được khởi tạo tại AuthApi"
      },
      "AuthUserInfo": {
        "Key": "Auth:UserInfo_{uname}_{appType}",
        "DistGroup": "Auth",
        "MemSeconds": 180,
        "DistSeconds": 14400,
        "Note": "Thông người dùng đăng nhập theo username"
      }
    }
  },
  "Authentication": {
    "Type": "ApiKey",
    "ApiKey": {
      "Default": {
        "Realm": "sample",
        "KeyName": "X-API-KEY",
        "Keys": [
          "ZofkXgxwiO6F2s1JJCX5L6Wa7JctPmpO"
        ]
      }
    },
    "Cognito": {
      "Endpoint": "https://cognito-idp.us-east-1.amazonaws.com",
      "Region": "us-east-1",
      "UserPools": {
        "key": "value"
      }
    }
  },
  "Middlewares": {
    "ApiResponseWrapper": {
      "IsEnable": true,
      "Ignores": {
        "PathStartWith": [
          "/swagger"
        ],
        "Path": [
          "/healthz"
        ]
      }
    }
  },
  "OTLP": {
    "Name": "sample-code",
    "Namespace": "sample-code-namespace",
    "Version": "1.1.1",
    "InstanceId": "hoangnh",
    "Attributes": {
      "k8s.node.name": "node-sample"
    },
    "Logging": {
      "IncludeConsoleExporter": true,
      "Exporter": "otlp",
      "Endpoint": "http://localhost:14317",
      "IncludeFormattedMessage": true,
      "IncludeScopes": true,
      "ParseStateValues": true
    },
    "Tracing": {
      "Exporter": "otlp",
      "Endpoint": "http://localhost:14317"
    },
    "Metric": {
      "Exporter": "otlp",
      "Endpoint": "http://localhost:14317"
    },
    "HistogramAggregation": "explicit",
    "AspNetCoreInstrumentation": {
      "RecordException": "true"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}